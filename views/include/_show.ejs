<%for(var i=0;i<users.length;i++){%>
<tr>
    <td><%=i+1%></td>
    <td><%=users[i].name%></td>
    <td><%=users[i].email%></td>
    <td><%=users[i].gender%></td>
    <td><%=users[i].status%></td>
    <td><a href="/update-user"><i class="fas fa-edit"></i></a></td>
    <td><a data-id=<%=users[i]._id%>><i class="fa-solid fa-trash-can"></i> </a></td>
</tr>
<%}%>